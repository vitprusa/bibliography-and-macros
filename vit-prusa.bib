@Article{	  rajagopal.kr:on*2,
  author	= "Rajagopal, K. R.",
  title		= "On implicit constitutive theories",
  language	= "English",
  journal	= "Appl. Math., Praha ",
  volume	= "48",
  number	= "4",
  pages		= "279-319",
  year		= "2003",
  doi		= {10.1023/A:1026062615145},
  abstract	= "{Summary: In classical constitutive models such as
		  Navier-Stokes fluid model, and the Hookean or neo-Hookean
		  solid models, the stress is given explicitly in terms of
		  kinematical quantities. Models for viscoelastic and
		  inelastic responses on the other hand are usually implicit
		  relationships between the stress and kinematical
		  quantities. Another class of problems wherein it would be
		  natural to develop implicit constitutive theories, though
		  seldom resorted to, are models for bodies that are
		  constrained. In general, for such materials the material
		  moduli that characterize the extra stress could depend on
		  the constraint reaction. (E.g., in an incompressible fluid,
		  the viscosity could depend on the constraint reaction
		  associated with the constraint of incompressibility. In the
		  linear case, this would be the pressure.) Here we discuss
		  such implicit constitutive theories. We also discuss a
		  class of bodies described by an implicit constitutive
		  relation for the specific Helmholtz potential that depends
		  on both the stress and strain, and which does not dissipate
		  in any admissible process. The stress in such a material is
		  not derivable from a potential, i.e., the body is not
		  hyperelastic (Green elastic).}",
  keywords	= "{constraint; Lagrange multiplier; Helmholtz potential;
		  rate of dissipation}",
  classmath	= "{*74A20 (Theory of constitutive functions) 76A02
		  (Foundations of fluid mechanics) 76A05 (Non-Newtonian
		  fluids) 76A10 (Viscoelastic fluids) }"
}

@Article{	  rajagopal.kr:on*3,
  author	= "Rajagopal, K. R.",
  title		= "On implicit constitutive theories for fluids",
  language	= "English",
  journal	= "J. Fluid Mech.",
  volume	= "550",
  pages		= "243-249",
  year		= "2006",
  doi		= {10.1017/S0022112005008025},
  abstract	= "{Summary: We consider generalizations of fluid models
		  wherein the fluid is assumed to be incompressible, but with
		  the viscosity depending on pressure. We show that a natural
		  setting for the development of such models is a class of
		  implicit constitutive relations, which, in addition to the
		  fluid model described here, provides a means for developing
		  other complex models for viscoelastic fluids which cannot
		  be set within the ambit of classical explicit constitutive
		  relations for the stress in terms of the histories of
		  appropriate kinematical variables.}",
  classmath	= "{*76A10 (Viscoelastic fluids) }"
}

