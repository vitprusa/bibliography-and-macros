@Article{	  nurmuhammad.a.muhammad.m.ea:double,
  title		= {Double exponential transformation in the Sinc-collocation
		  method for a boundary value problem with fourth-order
		  ordinary differential equation},
  journal	= {Journal of Computational and Applied Mathematics},
  volume	= {182},
  number	= {1},
