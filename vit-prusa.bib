@Article{	  wiest.jm.phan-thien.n:nonisothermal,
  title		= "Nonisothermal flow of polymer melts",
  journal	= "J. Non-Newton. Fluid Mech.",
  volume	= "27",
  number	= "3",
  pages		= "333--347",
  year		= "1988",
  doi		= "10.1016/0377-0257(88)85004-3",
  author	= "Wiest, J. M. and Phan-Thien, N.",
  abstract	= "The Curtiss-Bird theory for polymer melts is modified to
		  allow the temperature to be a function of time. The
		  resulting constitutive equation involves integrals over the
		  temperature history of the polymer. The predictions of this
		  constitutive equation for the inception of uniaxial
		  elongation with simultaneous cooling at a constant rate are
		  calculated and compared with the experimental data of
		  Matsumoto and Bogue. Qualitative agreement between the
		  theory and the data is obtained when the link tension
		  coefficient in the Curtiss-Bird theory is not equal to
		  zero, but such agreement cannot be obtained when this
		  coefficient is equal to zero. The extension of the theory
		  to include spatially varying temperature is also
		  discussed."
}

