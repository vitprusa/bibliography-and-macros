@Article{	  oreilly.om.srinivasa.ar:on,
  author	= "O'Reilly, Oliver M. and Srinivasa, Arun R.",
  title		= "On a decomposition of generalized constraint forces",
  language	= "English",
  journal	= "Proc. R. Soc. Lond., Ser. A, Math. Phys. Eng. Sci. ",
