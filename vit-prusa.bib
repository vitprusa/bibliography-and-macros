  title		= {A Pseudo-Arclength Continuation Method for Nonlinear
		  Eigenvalue Problems},
  author	= {Mittelmann, Hans D.},
  journal	= {SIAM Journal on Numerical Analysis},
  jstor_issuetitle={},
  volume	= {23},
