@Article{	  hajshirmohammadi.b.khonsari.mm:on,
  title		= "On the entropy of fatigue crack propagation",
  journal	= "Int. J. Fatigue",
  volume	= "133",
  pages		= "105413",
  year		= "2020",
  issn		= "0142-1123",
  doi		= "10.1016/j.ijfatigue.2019.105413",
  author	= "Hajshirmohammadi, B. and Khonsari, M. M.",
  keywords	= "Degradation entropy generation, crack propagation rate,
		  Paris law, cyclic fatigue",
  abstract	= "Entropy flow from the plastic zone ahead of a crack tip is
		  predicted analytically and measured experimentally for
		  stainless steel SS304. It is shown that crack propagation
		  rate is related to the entropy flow during cyclic loading
		  of a flat specimen in mode I. The results show that entropy
		  flow is a reliable parameter for the prediction of crack
		  propagation rate in both low- and high-speed regions. It is
		  also shown that the application of the degradation entropy
		  generation theorem provides a method for evaluating the
		  constant in Paris law in terms of entropy flow."
}

@Article{	  hajshirmohammadi.b.khonsari.mm:simple,
  title		= "A simple approach for predicting fatigue crack propagation
		  rate based on thermography",
  journal	= "Theoretical and Applied Fracture Mechanics",
  volume	= "107",
  pages		= "102534",
  year		= "2020",
  issn		= "0167-8442",
  doi		= "10.1016/j.tafmec.2020.102534",
  author	= "Hajshirmohammadi, B. and Khonsari, M. M.",
  keywords	= "Crack propagation rate, Thermography method, Cyclic
		  fatigue",
  abstract	= "A simple model based on realistic assumptions for
		  determining the fatigue crack propagation rate is
		  presented. It relies on the measurement of the temperature
		  evolution around the crack tip thermographically. The
		  approach is capable of predicting crack propagation speed
		  in both low- and high-growth rates. Results can be used to
		  determine the stress intensity factor."
}

@Article{	  haldenwang.p.labrosse.g.ea:chebyshev,
