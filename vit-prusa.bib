@Article{	  xiao.h.bruhns.ot.ea:elastoplasticity,
  author	= {Xiao, H. and Bruhns, O. T. and Meyers, A.},
  title		= {Elastoplasticity beyond small deformations},
  journal	= {Acta Mech.},
  year		= {2006},
  volume	= {182},
  number	= {1},
  pages		= {31--111},
  abstract	= {This article is concerned with some essential aspects of
		  representative ideas and viewpoints as well as developments
		  in formulations of rate-independent elastoplasticity beyond
		  the small deformation range. Kinematical and physical
		  foundations underlying the basic variables, and the
		  essential constitutive structures implied by the work
		  postulate are examined and discussed, pro and con, from an
		  integrated viewpoint based on certain coherent threads
		  running through various formulations. Their constitutive
		  and computational implications are elucidated and
		  contrasted by analysing the main structural features based
		  on their basic variables. Emphasis is placed on new issues
		  and new understanding, and on physically pertinent
		  variables and formulations.},
