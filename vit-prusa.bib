@Article{	  demir.h.akyildiz.ft:unsteady,
  title		= "Unsteady thermal convection of a non-{N}ewtonian fluid",
  journal	= "Int. J. Eng. Sci.",
  volume	= "38",
  number	= "17",
  pages		= "1923--1938",
  year		= "2000",
  doi		= "10.1016/S0020-7225(00)00011-2",
  author	= "Demir, H. and Akyildiz, F.T.",
  keywords	= "Criminale–Erickson–Filbey (CEF) model, Thermal
		  convection, Waissenberg number",
  abstract	= "Two-dimensional unsteady natural convection of a
		  non-linear fluid represented by
		  Criminale–Erickson–Filbey (CEF) fluid model in a square
		  cavity is studied. The governing vorticity and energy
		  transport equations are solved numerically. The
		  contributions of the elastic and shear rate-dependent
		  characteristics of the liquid to the non-Newtonian
		  behaviour are investigated. The effect of the Weissenberg
		  (which is a measure of the elasticity of the fluid) and
		  Rayleigh numbers on the temperature and streamline profiles
		  is delineated and this has been documented for the first
		  time."
}

