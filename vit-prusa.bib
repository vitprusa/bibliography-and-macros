@Article{	  rajagopal.kr.saccomandi.g:on*1,
  author	= "Rajagopal, K. R. and Saccomandi, Giuseppe",
  title		= "On internal constraints in continuum mechanics",
  language	= "English",
  year		= "2006",
  doi		= {10.1155/DENM/2006/18572},
  abstract	= "{Summary: In classical particle mechanics, it is well
		  understood that while working with nonholonomic and
		  nonideal constraints, one cannot expect that the constraint
		  be worklexss. It is curious that in continuum mechanics,
		  however, the implications of the result in classical
		  mechanics have not been clearly understood. In this paper,
		  we show that in dealing with the response of dissipative
		  systems, one cannot require that constraints do no work or
		  ignore the fact that the material response functions depend
		  on the constraint reaction. An example of this is the
		  viscosity of a fluid depending on the pressure.}",
  keywords	= "{pressure-dependent viscosity; dissipative systems;
		  material response functions}",
  classmath	= "{*74A99 (Generalities, etc. of continuum mechanics of
		  solids) 76A99 (Foundations, constitutive equations,
		  rheology) }",
  journal	= "Differential Equations and Nonlinear Mechanics",
  pages		= "1--12"
}

