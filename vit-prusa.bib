@Article{	  suzuki.y.ohnawa.m.ea:thermodynamically,
  author	= {Suzuki, Yukihito and Ohnawa, Masashi and Mori, Naofumi and
		  Kawashima, Shuichi},
  title		= {Thermodynamically consistent modeling for complex fluids
		  and mathematical analysis},
  journal	= {Mathematical Models and Methods in Applied Sciences},
  volume	= {31},
  number	= {10},
  pages		= {1919--1949},
  year		= {2021},
  doi		= {10.1142/S0218202521500421},
  abstract	= { The goal of this paper is to derive governing equations
		  for complex fluids in a thermodynamically consistent way so
		  that the conservation of energy and the increase of entropy
		  is guaranteed. The model is a system of first-order partial
		  differential equations on density, velocity, energy (or
		  equivalently temperature), and conformation tensor. A
		  barotropic model is also derived. In the one-dimensional
		  case, we express the barotropic model in the form of
		  hyperbolic balance laws, and show that it satisfies the
		  stability condition. Consequently, the global existence of
		  solutions around equilibrium states is proved and the
		  convergence rates is obtained. }
}

