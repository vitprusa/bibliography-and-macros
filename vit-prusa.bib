  issn		= {{1631-0721}},
  keywords	= {{fluid mechanics; asymptotic modelling in fluid dynamics;
		  thermal convection; geo-astro physical fluid dynamics;
		  magneto-hydrodynamics}},
  keywords-plus	= {{LEE WAVES; RIGOROUS DERIVATION; NUMERICAL-MODELS;
